{
  "name": "@alifd/scaffold-simple",
  "version": "0.1.0",
  "description": "使用 TypeScript，未使用任何 UI 库。",
  "dependencies": {
    "antd": "^4.3.5",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.1.0",
    "react": "^16.4.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.4.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@ice/spec": "^1.0.1",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.5",
    "babel-plugin-import-graphql": "^2.7.0",
    "build-plugin-antd": "^0.1.0",
    "eslint": "^6.0.1",
    "ice.js": "^1.0.0",
    "stylelint": "^13.0.0"
  },
  "scripts": {
    "start": "icejs start --config build.config.js",
    "build": "icejs build --config build.config.js",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx ./",
    "stylelint": "stylelint ./**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba-fusion/materials/tree/master/scaffolds/scaffold-simple"
  },
  "ideMode": {
    "name": "ice-react"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-react-v3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "private": true,
  "originTemplate": "@alifd/scaffold-simple"
